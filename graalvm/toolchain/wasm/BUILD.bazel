"Defines toolchain types for GraalWasm."

package(default_visibility = ["//visibility:public"])

# Toolchain type: `graalwasm`
#
# Describes a `wasm` binary made available as part of a GraalVM installation of any type. This
# tool may be installed via the GraalVM Updater or provided otherwise.
toolchain_type(name = "graalwasm")

alias(
    name = "toolchain_type",
    actual = "graalwasm",
)
