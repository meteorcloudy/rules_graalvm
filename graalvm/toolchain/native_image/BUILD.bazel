"Defines toolchain types for GraalVM Native Image."

package(default_visibility = ["//visibility:public"])

# Toolchain type: `native_image`
#
# Describes a `native-image` binary made available as part of a GraalVM installation of any type.
# This tool may be installed via the GraalVM Updater or provided otherwise.
toolchain_type(name = "native_image")

alias(
    name = "toolchain_type",
    actual = "native_image",
)
