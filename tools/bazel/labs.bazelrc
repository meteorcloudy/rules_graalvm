build:labs --experimental_platforms_api
build:labs --experimental_local_lockfree_output
build:labs --experimental_cc_shared_library
build:labs --experimental_disable_external_package

build:labs --experimental_use_windows_sandbox=auto
build:labs --incompatible_strict_action_env
